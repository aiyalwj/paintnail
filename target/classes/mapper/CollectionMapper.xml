<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwj.PaintNail.mapper.CollectionMapper">

    <resultMap id="BaseResultMap" type="com.lwj.PaintNail.entity.Collection_post">
            <id property="collectionId" column="Collection_id" jdbcType="VARCHAR"/>
            <result property="postId" column="Post_id" jdbcType="VARCHAR"/>
            <result property="userId" column="User_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        Collection_id,Post_id,User_id
    </sql>
    <insert id="CollectPost">
        INSERT INTO Collection values(#{Collection_id}, #{Post_id}, #{User_id})
    </insert>
</mapper>
